/***
* base element
***/

.weui-page {
  padding: 0;
  min-width: 100vw;
  max-width: 100vw;
  min-height: 100vh;
  background-color: var(--mc-black);
  color: var(--bc-white-gold);
  overflow-x: hidden;
  font-family: DurantCentra, PingFang-SC-BOLD, -apple-system-font, 'Helvetica Neue', Helvetica, sans-serif;
  font-weight: var(--fw-regular);
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.chip {
  flex-shrink: 0;
  flex-grow: 0;
  font-weight: normal;
  font-size: 24rpx;
  font-weight: var(--fw-regular);
  border-radius: 40rpx;
  padding: 14rpx 24rpx;
  background: rgba(255, 255, 255, 0.05);
}

.chip.selected {
  border: 1rpx solid var(--mc-light-grey);
  color: var(--mc-light-grey);
}

.chip.not-selected {
  border: 1rpx solid rgba(255, 255, 255, 0.05);
  color: var(--mc-dark-grey);
}


image.symbol {
  width: 26.66rpx;
  height: 26.66rpx;
  margin-right: 8rpx;
}

.tab-button {
  border: 1rpx solid var(--mc-black-grey);
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  flex-wrap: nowrap;
  padding: 20rpx;
  margin-right: 20rpx;
  color: var(--mc-mid-grey);
  flex-basis: 1;
  flex-grow: 1;
}

.tab-button image {
  width: 128rpx;
  height: 96rpx;
}

.tab-button text {
  padding-left: 20rpx;
  padding-right: 40rpx;
  font-weight: var(--fw-medium);
  font-size: 38rpx;
}

.tab-button.active {
  border: 1rpx solid var(--bc-white-gold);
  color: var(--bc-white-gold);
}

.tab-button .exclusive-tag {
  background: var(--ac-super-blue-ribbon-blue);
  padding: 10rpx;
  font-size: 24rpx;
}

.link {
  text-decoration: underline;
}

.btn {
  padding: 0 20rpx !important;
  height: 88rpx;
  line-height: 88rpx;
  border-radius: 44rpx;
  text-align: center;
  margin-left: 0 !important;
  margin-right: 0 !important;
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  background-color: transparent
}

.btn.dark {
  color: var(--bc-white-gold) !important;
  background-color: var(--mc-black);
}

.btn.outlined {
  color: var(--mc-black) !important;
  border: 1rpx solid var(--mc-black);
}
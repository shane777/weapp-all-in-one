  flex-direction: column;
  padding: 24px;
  box-sizing: border-box;
} 
page {
  --bc-blue-ribbon-black: #252627;
  --bc-white-gold: #F6F6F6;

  --ac-super-blue-ribbon-blue: #191EB4;
  --ac-blue-ribbon-blue: #182865;
  --ac-red: #D83516;
  --ac-green: #005648;
